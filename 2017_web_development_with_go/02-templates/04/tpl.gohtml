<html>
<body>
  {{range .}}
    <h1>{{.Name}} Menu</h1>
    <h2>Breakfast</h2>
    <ul>
    {{range .Breakfast}}
      <li>{{.Name}}   {{.Price}}</li>
    {{end}}
    </ul>

    <h2>Lunch</h2>
    <ul>
    {{range .Lunch}}
      <li>{{.Name}}   {{.Price}}</li>
    {{end}}
    </ul>

    <h2>Dinner</h2>
    <ul>
    {{range .Dinner}}
      <li>{{.Name}}   {{.Price}}</li>
    {{end}}
    </ul>
  {{end}}
</body>
</html>
